  "type": "module",
  "scripts": {
    "test": "yarn node --experimental-vm-modules $(yarn bin jest)",
    "fame-ipfs:origin:build": "esbuild ./src/lambda/fls-ipfs/origin.ts --inject:./deploy/lib/esbuild/cjs-shim.ts --external:aws-sdk --external:canvas --external:dtrace-provider --platform=node --format=esm --bundle --target=node24 --external:ipfs-http-client --outfile=./deploy/.layers/fls-ipfs/origin/index.mjs",
    "fame-thumb:build": "esbuild src/lambda/fame/thumb.ts --inject:./deploy/lib/esbuild/cjs-shim.ts --external:aws-sdk --external:canvas --external:dtrace-provider --platform=node --format=esm --target=node24 --bundle --external:canvas --external:ipfs-http-client --outfile=./deploy/.layers/fame-thumb/index.mjs",
    "fame-mosaic:build": "esbuild src/lambda/fame/mosaic.ts --inject:./deploy/lib/esbuild/cjs-shim.ts --external:aws-sdk --external:canvas --external:dtrace-provider --platform=node --format=esm --target=node24 --bundle --external:canvas  --outfile=./deploy/.layers/fame-mosaic/index.mjs",
    "discord:build": "yarn discord:interaction:build && yarn discord:deferred:build",
    "discord:interaction:build": "esbuild ./src/discord/lambda/interaction.ts --inject:./deploy/lib/esbuild/cjs-shim.ts --external:aws-sdk --external:canvas --external:dtrace-provider --platform=node --format=esm --target=node24 --bundle  --outfile=./deploy/.layers/discord-interaction/index.mjs",
    "discord:deferred:build": "esbuild src/discord/lambda/deferred.ts --inject:./deploy/lib/esbuild/cjs-shim.ts --external:aws-sdk --external:canvas --external:dtrace-provider --platform=node --format=esm --target=node24 --bundle  --outfile=./deploy/.layers/discord-deferred/index.mjs",
    "discord:fls:wrapped:build": "esbuild src/lambda/fls-wrapper-event/index.ts --inject:./deploy/lib/esbuild/cjs-shim.ts --external:aws-sdk --external:canvas --external:dtrace-provider --platform=node --format=esm --target=node24 --bundle --external:dtrace-provider  --outfile=./deploy/.layers/fls-wrapper-event/index.mjs",
    "discord:cli": "yarn discord:cli:build && node dist/discord-cli/index.mjs",
    "discord:cli:build": "esbuild src/discord/cli.ts --inject:./deploy/lib/esbuild/cjs-shim.ts --platform=node --format=esm --target=node24 --outfile=dist/discord-cli/index.mjs",
    "build": "yarn fame-ipfs:origin:build && yarn fame-thumb:build && yarn fame-mosaic:build && yarn discord:build && yarn discord:fls:wrapped:build && yarn discord:cli:build",
    "generate:types": "graphql-codegen --config codegen.yml",
    "nodets": "node --import 'data:text/javascript,import { register } from \"node:module\"; import { pathToFileURL } from \"node:url\"; register(\"ts-node/esm\", pathToFileURL(\"./\"));'",
    "types": "tsc --noEmit -p tsconfig.json"
  },
